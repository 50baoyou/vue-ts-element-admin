{
  "name": "vue-ts-element-admin",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --cache --fix --ignore-path .gitignore",
    "lint:stylelint": "stylelint \"**/*.{css,scss,vue}\" --fix --ignore-path .gitignore",
    "lint:lint-staged": "lint-staged",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "npm run lint",
      "npm run format"
    ],
    "*.{cjs,json}": [
      "prettier --write"
    ],
    "*.vue": [
      "npm run lint",
      "npm run lint:stylelint",
      "npm run format"
    ],
    "*.{scss,css}": [
      "npm run lint:stylelint",
      "npm run format"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.5",
    "element-plus": "^2.5.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@iconify-json/bi": "^1.1.23",
    "@iconify-json/ep": "^1.1.14",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@unocss/preset-icons": "^0.58.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "cz-git": "^1.8.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.33",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "sass": "^1.69.7",
    "stylelint": "^16.1.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.3.0",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.10",
    "vite-plugin-compression2": "^0.11.0",
    "vite-plugin-html": "^3.2.1",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-mock-dev-server": "^1.4.5",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^1.8.25"
  }
}
